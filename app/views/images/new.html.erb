<h1>Add Image</h1>

<%= form_for([@gallery, @image]) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.label :url %>
    <%= form.text_area :url %>
  </div>
  <%= form.submit "Add" %>
<% end %>


